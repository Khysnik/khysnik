  padding: 2px;
  padding-top: 3px;
  margin: 0;
}


// ranges
/////////////////////////////////////////////////////////////////////////////
// http://danielstern.ca/range.css/#/

range_track =
  width: 100%;
  height: 8px;
  cursor: pointer;

range_track_colors =
  background: #c0c0c0;
  border: 1px solid #808080;

range_track_focused =
  background: #cdcdcd;
  outline: 0 none;

input[type=range] {
  -webkit-appearance: none;
  background: transparent;
  padding: 0;
  margin: 0;
  /*margin: 0 2px;*/
  width: 100%;
  height: 22px;
  /*margin: 7px 0;*/
}
/*input[type=range]::-moz-focus-inner {
  border: 0;
}*/
input[type=range]::-moz-focus-outer {
  border: 0;
}
input[type=range]:focus {
  outline: 0 none;
}
input[type=range]::-webkit-slider-runnable-track {
  width: 100%;
  {range_track}
  {range_track_colors}
  {$inset_deep}
}

input[type=range]:focus::-webkit-slider-runnable-track {
  {range_track_focused}
}
input[type=range]::-moz-range-track {
  {range_track}
  {range_track_colors}
  {$inset_deep}
}

input[type=range]::-ms-track {
  {range_track}
  cursor: pointer;
  background: transparent;
  border-color: transparent;
  color: transparent;
}
input[type=range]::-ms-fill-lower {
  {range_track_colors}
  {$inset_deep}
}
input[type=range]::-ms-fill-upper {
  {range_track_colors}
  {$inset_deep}
}

input[type=range]:focus::-ms-fill-lower {
  {range_track_focused}
}
input[type=range]:focus::-ms-fill-upper {
  {range_track_focused}
}


range_thumb =
  height: 22px;
  width: 8px;
  cursor: pointer;
  border-radius: 0;
  background: #c0c0c0;
  background-color: #c0c0c0;

input[type=range]::-webkit-slider-thumb{
  -webkit-appearance: none;
  margin-top: -8px;
  {$outset_deep}
  {range_thumb}
}
input[type=range]::-moz-range-thumb{
  {$outset_deep}
  {range_thumb}
  height: 21px;
  width: 6px;
}
input[type=range]::-ms-thumb {
  {$outset_deep}
  {range_thumb}
  height: 8px;
}


/////////////////////////////////////////////////////////////////////////////

.skin_base {background: #c0c0c0;}
.skin_deep {background: #808080;}
.skin_deeper {background: #555;}
.skin_front {background: #999;}
.skin_fronter {background: #ccc;}
.skin_light {background: #fff;}
.skin_dark {background: #000;}
.skin_error {background: #861E1E;}


.skin_txt_base {color: #c0c0c0;}
.skin_txt_deep {color: #808080;}
.skin_txt_deeper {color: #555;}
.skin_txt_front {color: #999;}
.skin_txt_fronter {color: #ccc;}
.skin_txt_light {color: #fff;}
.skin_txt_dark {color: #000;}
.skin_txt_negative,
.skin_txt_error {color: #861E1E;}

.ui_alert--help .ui_alert__text {
  @extend .skin_light;
  @extend .skin_inset_deep;
  margin: 0;
  padding: 15px;
}


.skin_nerd {
  color: #c3ff00;
  background: #000;

  .ui_terminal {
    background: #000;
  }

  .ui_window {
    background: #000;
    border: 1px solid rgba(80, 105, 0, 1);
    box-shadow: 0 0 0 transparent;
  }

  .skin_inset {
    border: 1px solid rgba(80, 105, 0, 1); //1px dotted #c3ff00;
    box-shadow: 0 0 0 transparent;
  }

  button {
    color: #c3ff00;
    background-color: #000;
    border: 0 none;
    box-shadow: 0 0 0 transparent;
  }
  button.pressed {
    color: #fff;
    border: 0 none;
    box-shadow: 0 0 0 transparent;
  }
  .skin_scrollbar *::-webkit-scrollbar {
    height: 8px;
    width: 8px;
    background: #0D1811;
  }
  .skin_scrollbar *::-webkit-scrollbar-thumb {
    background: rgba(80, 105, 0, 1);
  }
  .skin_scrollbar *::-webkit-scrollbar-corner {
    background: rgba(80, 105, 0, 1);
  }
  .ui_hilit {
    background-color: #0D1811;
  }
  .sh_comment, .sh_comment span:not(.ui_log__unherit) {
    color: #4B512F;
  }
  .ui_log__green--light, .ui_log__green--light *:not(.ui_log__unherit) {
    color: #00A628;
  }
  .ui_log__green, .ui_log__green *:not(.ui_log__unherit) {
    color: #00A628;
  }
}

.skin_scrollbar *::-webkit-scrollbar {
  height: 8px;
  width: 8px;
  background: rgba(0,0,0,0.4);
}
.skin_scrollbar *::-webkit-scrollbar-thumb {
  background: rgba(125,125,125,0.3);
}
.skin_scrollbar *::-webkit-scrollbar-corner {
  background: rgba(125,125,125,0.3);
}

// menu
/////////////////////////////////////////////////////////////////////////////

[data-menuitem-selected=true] > .ui_form-ico-checkbox:after,
[data-menuitem-selected=true] > .ui_form-ico-radio:after {
  font-family: '_tomo', monospace;
  font-size: 8px;
  vertical-align: top;
}

.ui_menu--bar .ui_menu__item--opener:after,
.ui_menu--menu .ui_menu__item--opener:after,
.ui_menu--tree .ui_menu__item--opener:after {
  font-family: '_tomo', monospace;
  font-size: 8px;
  content: '▸';
}

.ui_menu--bar > ul > li.ui_menu__item--opener:after,
.ui_menu--tree .ui_menu__item--focus.ui_menu__item--opener:after,
.ui_menu--scroller > .ui_menu__down_handler:after {
  content: '▾';
  font-family: '_tomo', monospace;
  font-size: 8px;
  transform: none;
}
.ui_menu--scroller > .ui_menu__up_handler:after {
  content: '▴'; //'\25BE'; // ▾
  font-family: '_tomo', monospace;
  font-size: 8px;
  transform: none;
}


.ui_menu--bar > ul > li.ui_menu__item--opener:after {
  content: '';
  display: none;
}
.ui_menu--bar > ul > .ui_menu__item--opener {
  padding-right: 0px;
}
.ui_menu--bar > ul > .ui_menu__item > .ui_menu__item__text {
  padding: 2px 3px;
  padding-left: 2px;
}

// explorer
/////////////////////////////////////////////////////////////////////////////

.ui_explorer__nav__input {
  border-left: 0 none !important;
  border-right: 0 none !important;
  box-shadow: none !important;
}

.ui_explorer__nav {
  margin-bottom: 1px;
}



.ui_progress {
  font-family: _tomo;
  background: #aaa;
}
.ui_progress__bar {
  background: #020282;
  background: linear-gradient(90deg, #020282 8px, transparent 8px);
  background-size: 10px 50px;
}
.ui_progress__hint {
  font-family: _tomo;
  color: #fff;
  text-shadow:
        0 1px 0 #444,
       0 -1px 0 #444,
       -1px 0 0 #444,
        1px 0 0 #444,
    -1px -1px 0 #444,
     1px -1px 0 #444,
     -1px 1px 0 #444,
      1px 1px 0 #444;
}