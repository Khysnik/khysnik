.cursor_default {
  cursor: url('/c/sys/cursors/default.cur'), default;
}
.cursor_minicross {
  cursor: url('/c/sys/cursors/minicross.cur'), crosshair;
}
.cursor_cross {
  cursor: url('/c/sys/cursors/cross.cur'), crosshair;
}
.cursor_crosshair {
  cursor: url('/c/sys/cursors/crosshair.cur'), crosshair;
}
.cursor_pointer {
  cursor: url('/c/sys/cursors/pointer.cur'), pointer;
}
.cursor_metal {
  cursor: url('/c/sys/cursors/metal.cur'), pointer;
}
.cursor_text {
  cursor: url('/c/sys/cursors/text.cur'), text;
}
.cursor_move {
  cursor: url('/c/sys/cursors/move.cur'), move;
}
.cursor_not-allowed {
  cursor: url('/c/sys/cursors/not-allowed.cur'), not-allowed;
}

.ui_dropzone, .ui_dropzone * {
  cursor: url('/c/sys/cursors/drag.cur'), grabbing !important;
}
.ui_dropzone--move, .ui_dropzone--move *,
.ui_dropzone--droppable, .ui_dropzone--droppable * {
  cursor: url('/c/sys/cursors/drag-move.cur'), grabbing !important;
}
.ui_dropzone--copy, .ui_dropzone--copy * {
  cursor: url('/c/sys/cursors/drag-copy.cur'), copy !important;
}
.ui_dropzone--link, .ui_dropzone--link * {
  cursor: url('/c/sys/cursors/drag-link.cur'), alias !important;
}
.ui_dropzone--no-drop, .ui_dropzone--no-drop * {
  cursor: url('/c/sys/cursors/drag-no-drop.cur'), no-drop !important;
}

.app_corglitch,
.ui_notif span,
.ui_menu .ui_menu__item,
.ui_icon > img,
.ui_icon > .ico,
.ui_icon > span,
//.ui_form label,
input[type="checkbox"],
input[type="radio"],
button,
a {
  @extend .cursor_pointer;
}

input, textarea {
  @extend .cursor_text;
}
label {
  @extend .cursor_default;
}

html > body *[disabled] {
  @extend .cursor_not-allowed;
}

.ui_window--draggable {
  @extend .cursor_move;
}

.ui_menu--scroller > .ui_menu__up_handler:disabled,
.ui_menu--scroller > .ui_menu__down_handler:disabled,
.ui_menu .ui_menu__item--disabled {
  @extend .cursor_default;
}

@-webkit-keyframes blinker {
  0% { visibility: visible; }
  50% { visibility: hidden; }
  100% { visibility: visible; }
}
@keyframes blinker {
  0% { visibility: visible; }
  50% { visibility: hidden; }
  100% { visibility: visible; }
}
blink {
  -webkit-animation: blinker steps(1) 1s infinite;
      animation: blinker steps(1) 1s infinite;
}

#s42_bios {
  overflow-y: auto;
}
@keyframes displayBiosError {
  0% { opacity: 0; height: 0; max-height: 0; line-height:0; }
  99.99% { opacity: 0; height: 0; max-height: 0; line-height:0; }
  100% { opacity: 1; height: 16px; max-height: 16px; line-height:1; }
}
#s42_bios .delay0, #s42_bios .delay0 * {
  animation: displayBiosError 0.3s;
}
#s42_bios .delay1, #s42_bios .delay1 * {
  animation: displayBiosError 1s;
}
#s42_bios .delay2, #s42_bios .delay2 * {
  animation: displayBiosError 15s;
}
#s42_bios .delay3, #s42_bios .delay3 * {
  animation: displayBiosError 25s;
}

#s42_bios > blink {
  display: block;
  overflow: hidden;
}

